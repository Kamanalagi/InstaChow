<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.group2.instachow">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.group2.instachow.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.group2.instachow.LoginOptionsPage" />
        <activity android:name="com.example.group2.instachow.SignInPage" />
        <activity android:name="com.example.group2.instachow.SignUpPage" />
        <activity
            android:name="com.example.group2.instachow.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme" />
        <activity android:name="com.example.group2.instachow.OrderPage"></activity>
    </application>


</manifest>